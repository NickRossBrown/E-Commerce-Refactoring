<div class="product-edit" id="projuct-<%= @product.id %>-edit">
<%= form_for @product, remote: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  
  <%= f.label :description%>
  <%= f.text_field :description%>
  
  <%= f.label :price%>
  <%= f.number_field :price  %>
 
  <%= f.submit %>
<% end %>